pipeline{
    agent any
    tools{
        maven 'M2_HOME'
    }
    stages{
         stage('Sonarqube scan'){
            steps{
                withSonarQubeEnv('sonarserver') {
                sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=mycaro2007_week16-code'
                }
            }
        }
        stage('all maven commands'){
            steps{
                sh 'mvn clean test compile install package'
                 }
           }
       stage('upload artifact'){
           steps {
              sh 'curl -uadmin:AP3oJAo4EgQpQnCMyCyepW1iVFf -T target/bio*.jar "http://44.204.232.31:8081/artifactory/geoapp/"'
            } 
        }
        }

    }
