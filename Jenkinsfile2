pipeline{
    agent any
    tools{
        maven 'M2_HOME'
    }
    stages {
         stage('Sonarqube scan'){
            steps{
                sh 'mvn sonar:sonar'
            }
        }
        stage('all all maven commands'){
            steps{
                sh 'mvn clean test compile install package'
            }
        }
       
    }

}